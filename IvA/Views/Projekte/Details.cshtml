@model IvA.Models.ProjekteModel

@{
    ViewData["Title"] = "Projektdetails";
}

<h1>Übersicht über die Projektdetails</h1>

<div>
    <h4>Projekte</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Projektname)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Projektname)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Projektersteller)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Projektersteller)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ErstelltAm)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ErstelltAm)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Mitglieder)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Mitglieder)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Beschreibung)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Beschreibung)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Deadline)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Deadline)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Status)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.ProjekteId">Anpassen</a>
    <a class="btn btn-primary" asp-action="Index">Zurück zur Projektübersicht</a>
</div>
